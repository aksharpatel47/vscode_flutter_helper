{
  "name": "vscode-flutter-helper",
  "displayName": "Flutter Helpers",
  "description": "Helper utilities for flutter projects",
  "version": "0.0.1",
  "publisher": "aksharpatel47",
  "icon": "media/flutter.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/aksharpatel47/vscode_flutter_helper"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flutterHelper.genModel",
    "onCommand:flutterHelper.genModelWatch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutterHelper.genModel",
        "title": "Flutter Helper: Generate JSONSerializable Model Code"
      },
      {
        "command": "flutterHelper.genModelWatch",
        "title": "Flutter Helper: Generate JSONSerializable Model Code & Watch For Changes"
      }
    ],
    "snippets": [
      {
        "language": "dart",
        "path": "./dart.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  }
}
